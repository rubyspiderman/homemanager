<% if @finish.errors.any? %>
	
	prepareEditForm('finish', "<%= escape_javascript(render(:partial => "form"))%>");

<% else %>

	closeDataForm();
	updateDataTable(
		"<%= escape_javascript(render(:partial => "shared/resource_index", :locals => { :resources => @finishes })) %>",
		"<%= @finishes.count %>");

<% end %>